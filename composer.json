{
    "name": "xhyperf/logger-plus",
    "description": "",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "logger"
    ],
    "homepage": "https://yangqi.co",
    "support": {
        "issues": "https://github.com/xhyperf/logger-plus/issues",
        "source": "https://github.com/xhyperf/logger-plus",
        "docs": "https://yangqi.co",
        "pull-request": "https://github.com/xhyperf/logger-plus/pulls"
    },
    "require": {
        "php": ">=8.1",
        "hyperf/collection": "~3.1",
        "hyperf/di": "~3.1",
